package org.mortgages

import org.mortgages.LoanApplication
import org.mortgages.Applicant

rule "Underage"
	salience 10
	dialect "mvel"
	when
		application : LoanApplication( )
		Applicant( age < "21" )
	then
		application.setApproved( false );
		application.setExplanation( "Underage" );
		retract( application );
end
